<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
            fx:controller="UI.Controller.ShopServiceOverviewController">
    <children>
        <SplitPane dividerPositions="0.29797979797979796" prefHeight="300.0" prefWidth="600.0">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="304.0" prefWidth="238.0">
                    <children>
                        <TableView fx:id="shopServicesTable" prefHeight="298.0" prefWidth="175.0"
                                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Nazwa"/>
                                <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Cena"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </AnchorPane>
                <AnchorPane>
                    <children>
                        <Label layoutX="14.0" layoutY="14.0" text="Detale"/>
                        <GridPane layoutX="14.0" layoutY="31.0">
                            <children>
                                <Label text="Nazwa"/>
                                <Label fx:id="nameLabel" text="Label" GridPane.columnIndex="1"/>
                                <Label text="Cena" GridPane.rowIndex="1"/>
                                <Label fx:id="priceLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <padding>
                                <Insets top="10.0"/>
                            </padding>
                        </GridPane>
                        <AnchorPane layoutX="253.0" layoutY="258.0" prefHeight="26.0" prefWidth="150.0">
                            <children>
                                <FlowPane hgap="5.0" prefHeight="70.0" prefWidth="161.0" vgap="5.0"
                                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#handleNewShopService" text="Create"/>
                                        <Button mnemonicParsing="false" onAction="#handleEditShopService" text="Edit"/>
                                        <Button mnemonicParsing="false" onAction="#handleDeleteShopService"
                                                text="Delete"/>
                                    </children>
                                </FlowPane>
                            </children>
                        </AnchorPane>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
